 if fname == "supcalc":
    a = int(input("Введите число 1\n"))
    b = int(input("Введите число 2\n"))
    c = input("Введите операцию\n")
    if c == "*":
         print(a*b)
    elif c == "/":
        if b == 0:
            print("На ноль делить нельзя")
        else:
            print(a/b)
    elif c == "+":
         print(a+b)
    elif c == "-":
         print(a-b)
    else:
        print("Некорректаня операция")

