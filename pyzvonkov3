 if fname == "mostmin5":
     # наибольшее и наименьшее число из 5 данных
     a = float(input(""))
     b = float(input(""))
     c = float(input(""))
     d = float(input(""))
     e = float(input(""))
     if a>b and a>c and a>d and a>e:
        print("Наибольшее =",a)
     if b > a and b > c and b > d and b > e:
        print("Наибольшее =", b)
     if c>b and c>a and c>d and c>e:
        print("Наибольшее =",c)
     if d > b and d > c and d > a and d > e:
        print("Наибольшее =", d)
     if e > b and e > c and e > d and e > a:
        print("Наибольшее =", e)
     if a<b and a<c and a<d and a<e:
        print("Наименьшее =",a)
     if b < a and b < c and b < d and b < e:
        print("Наименьшее =", b)
     if c<b and c<a and c<d and c<e:
        print("Наименьшее =",c)
     if d < b and d < c and d < a and d < e:
        print("Наименьшее =", d)
     if e < b and e < c and e < d and e < a:
        print("Наименьшее =", e)

 if fname == "sorting3":
     # сортировка трех чисел от большего к меньшему
     a = float(input(""))
     b = float(input(""))
     c = float(input(""))
     if a < b < c:
         print(a, b, c, sep="\n")
     if a < c < b:
         print(a, c, b, sep="\n")
     if b < a < c:
         print(b, a, c, sep="\n")
     if b < c < a:
         print(b, c, a, sep="\n")
     if c < b < a:
         print(c, b, a, sep="\n")
     if c < a < b:
         print(c, a, b, sep="\n")

 if fname == "interesting945":
     # признание трехзначного числа интересным если разность наибольшей и наименьшей цифры равна средней
     a = float(input(""))
     b = a//100
     c = a//10-b*10
     d = a%10
     nums = [b, c, d]
     maxdigit = max(nums)
     mindigit = min(nums)
     nums.remove(maxdigit)
     nums.remove(mindigit)
     if maxdigit - mindigit == nums[0]:
         print("Интересное")
     else:
         print("Неинтересное")

