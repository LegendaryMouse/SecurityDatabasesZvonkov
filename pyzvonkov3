 '''мне нужно было написать код который по 2 заданым числам находит средние числа'''
 if fname == "awgnums":
     a = float(input())
     b = float(input())
     c = 0.5
     print('Арифметик ', (a + b) / 2)
     print('Геометрик ', (a * b) ** c)
     print('гармоник ', (a * b * 2) / (a + b))
     print('Квадрат', (((a ** 2) + (b ** 2)) / 2) ** c)

 '''мне нужно было написать код вычесляющий тригонометрическое вырожение по заданному х'''
 if fname == "trigonometrymath":
     import math

     x = float(input())
     sin1 = math.sin(x)
     cos1 = math.cos(x)
     tan1 = math.tan(x) ** 2
     print('sin x = ', sin1)
     print('cos x = ', cos1)
     print('tan^2 x = ', tan1)
     r = (x * math.pi) / 180
     print('r = ', r)

 '''мне нужно было написать код который находит площадь правильного многоугольника'''
 if fname == "squarenotsquare":
     import math

     a = float(input())
     b = float(input())
     s = (a * (b ** 2) / (4 * math.tan(math.pi / a)))
     print(s)

coolallfunction(input("Какую функцию мне запустить?\n"))


